{
  "crons": [
    {
      "path": "/api/shepartiststat",
      "schedule": "0 21 * * *",
      "headers": {
        "x-cron-secret": "@artist_cron_secret"
      }
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/((?!api).*)", "destination": "/" }
  ]
}
